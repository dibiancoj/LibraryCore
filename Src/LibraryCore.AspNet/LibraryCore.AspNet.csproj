	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\LibraryCore.JsonNet\LibraryCore.JsonNet.csproj" />
	</ItemGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

</Project>
